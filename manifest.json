{

  "manifest_version": 2,

  "name": "dswitch",
  "version": "0.1.3",
  "description": "dmenu inspired tab switcher.",

  "permissions": [
    "tabs"
  ],

  "web_accessible_resources": [
    "modal.html"
  ],

  "content_scripts": [
    {
      "matches": [ "*://*/*" ],
      "js": [ "content.js" ],
      "css": [ "style.css" ]
    }
  ],

  "background": {
    "scripts": [ "background.js" ],
    "persistent": false
  }

}
